{
  "name": "ccd-admin-web",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^16.15.0",
    "yarn": "^1.12.3"
  },
  "scripts": {
    "setup": "gulp sass copy-files",
    "start": "node server.js",
    "lint": "tslint --project tsconfig.json --type-check && sass-lint -v -q",
    "sass": "gulp sass copy-files",
    "watch": "gulp develop",
    "test": "NODE_ENV=mocha LOG_LEVEL=OFF mocha --compilers ts:ts-node/register -r tsconfig-paths/register $(find src/test \\( -name '*.ts' -or -name '*.js' \\) ! -path '*a11y*') --reporter mochawesome --reporter-options reportFilename=unit,inlineAssets=true,reportTitle=ccd-admin-web-unit",
    "test:routes": "NODE_ENV=mocha LOG_LEVEL=OFF mocha --compilers ts:ts-node/register -r tsconfig-paths/register $(find src/test \\( -name '*.ts' -or -name '*.js' \\) -path '*/routes/*') --reporter mochawesome --reporter-options reportFilename=routes,inlineAssets=true,reportTitle=ccd-admin-web-routes",
    "test:a11y": "NODE_PATH=.",
    "test:unit": "NODE_ENV=test NODE_PATH=. mocha 'src/test/**/*.spec.js' --reporter spec --recursive",
    "test:coverage": "NODE_ENV=mocha LOG_LEVEL=OFF nyc mocha --compilers ts:ts-node/register -r tsconfig-paths/register $(find src/test \\( -name '*.ts' -or -name '*.js' \\) ! -path '*a11y*')",
    "test:smoke": "echo SMOKE TESTS NOT CONFIGURED",
    "test:functional": "echo 'TODO - FUNCTIONAL TEST'",
    "fortifyScan": "./src/test/java/gradlew -p src/test/java fortifyScan",
    "sonar-scan": "NODE_PATH=. sonar-scanner -X -Dsonar.projectKey='ccd-admin-web' -Dsonar.sources=src/main/ -Dsonar.tests=src/test/ -Dsonar.exclusions=node_modules/** -Dsonar.javascript.lcov.reportPaths=coverage/lcov.info",
    "snyk-protect": "echo 'snyk protect'",
    "prepare": "echo 'npm run snyk-protect'",
    "prepublish": "echo 'npm run snyk-protect'"
  },
