  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/main"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/main/public",
      "src/main/modules",
      "src/main/router",
      "src/main/util",
      "src/main/app.ts",
      "src/main/server.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "coverage",
    "all": true
  },
  "snyk": true
}
